{
    "craftObject:<Type>Pickaxe":{
        "craftpos:00":"<Type>",
        "craftpos:10":"<Type>",
        "craftpos:20":"<Type>",
        "craftpos:11":"stick",
        "craftpos:21":"stick"
    },
    "craftObject:<Type>Axe":{
        "craftpos:00":"<Type>",
        "craftpos:10":"<Type>",
        "craftpos:01":"<Type>",
        "craftpos:11":"stick",
        "craftpos:21":"stick"
    },
    "craftObject:woodenPlank":{
        "craftpos:00":"woodInv"
    },
    "craftObject:stick":{
        "craftpos:00":"woodenPlankInv",
        "craftpos:01":"woodenPlankInv"
    },
    "craftObject:craftingBenchInv":{
        "craftpos:00":"woodenPlankInv",
        "craftpos:01":"woodenPlankInv",
        "craftpos:10":"woodenPlankInv",
        "craftpos:11":"woodenPlankInv"
    },
    "craftObject:furnaceInv":{
        "craftpos:00":"cobblestoneInv",
        "craftpos:01":"cobblestoneInv",
        "craftpos:10":"cobblestoneInv",
        "craftpos:11":"cobblestoneInv"
    },
    "craftObject:furnaceInsertIron":{
        "craftpos:0":"ironOreInv"
    },
    "craftObject:furnaceInsertCoal":{
        "craftpos:1":"coal"
    },





    "accessResource:craftingBench":{
        "interaction:craftingBench:open":1,
        "movement:lookAt{craftingBench}":"closest",
        "or":[
            {"placeObject:craftingBenchInv":1,"acquireResource:craftingBenchInv":1},
            {"moveToLocation":"benchLoc","locateObject:craftingBench":"benchLoc"}
        ]
    },
    "accessResource:playerInventory":{
        "interaction:playerInventory:open":1,
    },
    "accessResource:3x3Crafting":{
        "accessResource:craftingBench":1
    },
    "accessResource:2x2Crafting":{
        "or":[
            {"accessResource:craftingBench":1},
            {"accessResource:playerInventory":1}
        ]
    },
    "accessResource:furnace":{
        "interaction:furnace:open":1,
        "movement:lookAt{furnace}":"closest",
        "or":[
            {"placeObject:furnaceInv":1,"acquireResource:furnaceInv":1},
            {"moveToLocation":"furnaceLoc","locateObject:furnace":"furnaceLoc"}
        ]
    },



    "acquireResource:stick":{
        "returns":4,
        "craftObject:stick":1,
        "accessResource:2x2Crafting":1,
        "acquireResource:woodPlankInv":2
    },
    "acquireResource:woodPlankInv":{
        "returns":4,
        "craftObject:woodPlankInv":1,
        "accessResource:2x2Crafting":1,
        "acquireResource:{wood}Block":1
    },

    "acquireResource:<Type>Block":{
        "returns":1,
        "movement:stepBackward":1,
        "movement:stepForward":1,
        "harvestResource:<Type>":1,
        "movement:lookAt<Type>":"closest",
        "moveToLocation":"<Type>Loc",
        "locateObject:<Type>":"<Type>Loc",
        "acquireResource:{<toolEdge>}Pickaxe":1,
        "setvar:<toolEdge>":"toolChooser(<Type>.toolType,level>=<Type>.minimumToolLevel)"
    },

    "acquireResource:furnaceInv":{
        "returns":1,
        "craftObject:furnace":1,
        "accessResource:2x2Crafting":1,
        "acquireResource:{stone}Block":4
    },
    "acquireResource:iron":{
        "returns":8,
        "craftObject:furnaceRetrieive:iron":8,
        "interaction:wait{10000}":8,
        "craftObject:furnaceInsertIron":8,
        "craftObject:furnaceInsertCoal":1,
        "accessResource:furnace":1,
        "parallel":{
            "acquireResource:{ironOre}Block":8,
            "acquireResource:{coalOre}Block":1,
        }
    },

    "acquireResource:<Type>Pickaxe":{
        "returns":1,
        "craftObject:<Type>Pickaxe":1,
        "accessResource:3x3Crafting":1,
        "parallel":{
            "acquireResource:stick":2,"acquireResource:<Type>":3
        }
    },



    "harvestResource:<Type>":{
        "key:leftClick":"hold:<x>",
        "setvar:x":"<Type>:breakTime[<tool>.level]",
        "switchToObject":"<tool>",
        "setvar:tool":"toolChooser(<Type>.toolType,level>=<Type>.minimumToolLevel)"
    },


    "placeObject:<Type>":{
        "key:turnUp":90,
        "key:rightClick":"click",
        "key:space":"click",
        "key:turnDown":90,
        "switchToObject:<Type>":1
    },


    "locateObject<Type>":{

    },

    "movement:stepForward":{

    },
    "movement:stepBackward":{

    },
    "movement:lookAt<Type>":{


    },


    "interaction:wait<Type>":{
        "wait":"<Type>"
    },
    "interaction:furnace:open":{
        "key:rightClick":"click"
    },
    "interaction:craftingBench:open":{
        "key:rightClick":"click"
    },
    "interaction:playerInventory:open":{
        "key:rightClick":"click"
    }
}
